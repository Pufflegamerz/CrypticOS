arr a 10 0

; In order to dynamically retrieve/set a certain cell in memory,
; we must include a kernel function in the program.
inc "$kernel/main.casm"
jmp a
	inc "$kernel/mov.casm"
a:

; mem[gb] = gc
set ga SET
set gb [a]
set gc '2' ; value to set
run k.mov

; gr = mem[gb]
set ga GET
set gb [a]
run k.mov

; Debug char
inl "#"
